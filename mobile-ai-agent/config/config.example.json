{
  "agent": {
    "name": "BugBountyBot",
    "mode": "auto",
    "max_concurrent_scans": 3
  },
  "notification": {
    "telegram_enabled": false,
    "telegram_bot_token": "YOUR_BOT_TOKEN_HERE",
    "telegram_chat_id": "YOUR_CHAT_ID_HERE",
    "email_enabled": false,
    "email_to": "your@email.com",
    "smtp_server": "smtp.gmail.com",
    "smtp_port": 587,
    "smtp_user": "your@gmail.com",
    "smtp_password": "YOUR_APP_PASSWORD_HERE"
  },
  "targets": [
    "example.com"
  ],
  "scan_interval_hours": 12,
  "tools": {
    "subfinder": false,
    "httpx": false,
    "nuclei": false,
    "api_based": true
  },
  "api_keys": {
    "securitytrails": "",
    "shodan": "",
    "virustotal": "",
    "censys_id": "",
    "censys_secret": ""
  },
  "github": {
    "auto_commit": false,
    "repository": "username/repo",
    "branch": "main",
    "token": ""
  },
  "scan_options": {
    "timeout_seconds": 300,
    "max_subdomains_to_probe": 100,
    "follow_redirects": true,
    "verify_ssl": false
  }
}
